		<queryString>
			<![CDATA[SELECT PCP_RECURSO.NO_RECURSO
FROM tb_pcp_operacao PCP_OPERACAO
    INNER JOIN tb_pcp_recurso PCP_RECURSO
        ON PCP_OPERACAO.CO_RECURSO = PCP_RECURSO.CO_RECURSO
WHERE PCP_OPERACAO.CO_PRODUTO = '$P{CO_PRODUTO}'
ORDER BY PCP_OPERACAO.CO_OPERACAO]]>
		</queryString>
