	<queryString>
		<![CDATA[SELECT SUM(ETIQUETA.QTD_PRODUZIR) TOTAL,
	ETIQUETA.*,
	UPPER(ETIQUETA.DS_PRODUTO),
	CONCAT(UPPER(ETIQUETA.CO_INT_PRODUTO),' - ',UPPER(ETIQUETA.NO_COR),' - ',ETIQUETA.NU_ESPESSURA) CO_INTERNO_COR_ESP,
	DATE_FORMAT(ETIQUETA.DT_EMISSAO,'%m/%d/%Y') DT_EMISSAO
